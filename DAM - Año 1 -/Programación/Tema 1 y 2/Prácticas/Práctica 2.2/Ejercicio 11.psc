Proceso P2_2_11
	
	Definir a,b,c Como Entero;
	
	Escribir "Introduce el valor A";
	Leer a;
	Escribir "Introduce el valor B";
	Leer b;
	
	//Variables
	c <- 0;
	
	//Instrucciones
	c <- c+a; 
	a <- b;
	b <- c;
	
	
	//Final
	Escribir "El nuevo valor de A es el siguiente ",a;
	Escribir "El nuevo valor de B es el siguiente ",b;
	
FinProceso
